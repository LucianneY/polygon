# Generate a plot and save as an image
df <- read.csv("XE_region1_centroid.csv")
g <- ggplot(df, aes(x = x_centroid, y = y_centroid)) +
geom_point(alpha = 0.6, size = 0.1) +
theme_minimal()
g
g <- ggplot(df, aes(x = x_centroid, y = y_centroid)) +
geom_point(size = 0.01) +
coord_fixed()+
theme_minimal()
g
ggsave("scatterplot.png", plot = g, device = "png", width = 10, height = 7, dpi = 300)
